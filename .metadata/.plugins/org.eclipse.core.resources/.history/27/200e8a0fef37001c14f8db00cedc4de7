 * See https://www.eclipse.org/Xtext/documentation/303_runtime_concepts.html#code-generation
 */
class DslCsvGenerator extends AbstractGenerator {	
	override void doGenerate(Resource resource, IFileSystemAccess2 fsa, IGeneratorContext context) {
//		fsa.generateFile('greetings.txt', 'People to greet: ' + 
//			resource.allContents
//				.filter(Greeting)
//				.map[name]
//				.join(', '))

		val c = resource.allContents.head as Program
		val bash = new ASTtoBash()
		println(bash.eval(c.expr))
		val pp = new ASTtoPP()
		println(pp.eval(c.expr))
		val interp = new ASTtoInterpretation()
		println(interp.eval(c.expr, new InterpretationContext()))
	}
}

